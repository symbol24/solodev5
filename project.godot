; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Tower Offense"
config/description="The Dark is hunting the last source of light."
run/main_scene="res://Scenes/Worlds/boot.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Signals="*res://Code/Autoloads/signals.gd"
SM="*res://Scenes/Autoloads/scene_manager.tscn"
Game="*res://Scenes/Autoloads/game_manager.tscn"
Debug="*res://Scenes/Autoloads/debug.tscn"
Audio="*res://addons/AudioBySymbol/Code/Autoloads/audio_autoload.gd"

[debug]

gdscript/warnings/unused_parameter=0
gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/AudioBySymbol/plugin.cfg", "res://addons/kanban_tasks/plugin.cfg")

[file_customization]

folder_colors={
"res://Audio/": "blue",
"res://Code/": "yellow",
"res://Data/": "purple",
"res://Scenes/": "red",
"res://Textures/": "green"
}

[global_group]

light=""

[gui]

theme/custom="res://Data/default_theme.tres"
theme/custom_font="res://Data/Ui/Pixeletter.ttf"

